{
  "name": "wenbinye/tars",
  "description": "tars php server",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Ye Wenbin",
      "email": "wenbinye@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "wenbinye\\tars\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "wenbinye\\tars\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "doctrine/annotations": "^1.8",
    "php-di/phpdoc-reader": "^2.1",
    "php-di/php-di": "^6.0",
    "psr/container": "^1.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-server-handler": "^1.0",
    "psr/simple-cache": "^1.0",
    "kuiper/kuiper": "0.5.0.x-dev",
    "monolog/monolog": "^2.0",
    "symfony/console": "^5.0",
    "symfony/event-dispatcher": "^5.0",
    "symfony/finder": "^5.0",
    "symfony/validator": "^5.0",
    "webmozart/assert": "^1.6",
    "laminas/laminas-code": "^3.4",
    "laminas/laminas-diactoros": "^2.2"
  },
  "require-dev": {
    "ext-sockets": "*",
    "copyrenzhe/tars-ide-helper": "dev-master",
    "laravel/lumen-framework": "^7.1",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.4",
    "slim/slim": "^4.3",
    "swoft/swoole-ide-helper": "^4.4",
    "vlucas/phpdotenv": "^4.1",
    "wenbinye/tars-gen": "^0.1",
    "winwin/composer-phpcs-plugin": "^0.1"
  },
  "scripts": {
    "gen": "./vendor/bin/tars-gen"
  },
  "extra": {
    "kuiper": {
      "component-scan": ["wenbinye\\tars\\server\\framework"]
    },
    "tars": {
      "generator": {
        "client": {
          "tars_path": "tars",
          "servants": {
            "StatF": "tars.tarsstat.StatObj",
            "PropertyF": "tars.tarsproperty.PropertyObj",
            "ServerF": "tars.tarsnode.ServerObj",
            "Log": "tars.tarslog.LogObj",
            "QueryF": "tars.tarsregistry.QueryObj",
            "Config": "tars.tarsconfig.ConfigObj"
          }
        }
      }
    }
  }
}
